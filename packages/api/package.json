{
  "name": "@jetkit/api",
  "version": "1.0.1",
  "module": "build/index.js",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jetbridge/jetkit-react.git"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-jwt": "^1.2.0"
  },
  "scripts": {
    "test-ci": "CI=1 react-scripts test",
    "test": "react-scripts test",
    "build": "tsc",
    "start": "rm -rf build && tsc -w",
